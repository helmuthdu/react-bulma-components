{
  "name": "react-bulma-components",
  "version": "3.0.1-3",
  "description": "React components for Bulma framework",
  "main": "dist",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "eslint . && jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "clean:build": "rm -rf dist && rm -rf lib && rm -rf full",
    "build-storybook-static": "build-storybook -c .storybook -o .out",
    "prepare": "npm run clean:build && npm run build && npm run clean:test",
    "deploy-storybook": "storybook-to-ghpages",
    "build": "WEBPACK_ENV=INCLUDE_CSS BABEL_ENV=production NODE_ENV=production webpack --progress --color --mode=production && webpack --progress --color --mode=production && npm run create:lib",
    "create:lib": "NODE_ENV=production tsc --resolveJsonModule --declaration --esModuleInterop --jsx preserve --strict --target es5 --module commonjs --outDir lib src/index.ts",
    "clean:test": "rm -rf $(find ./lib -name \"__tests__\")"
  },
  "storybook-deployer": {
    "gitUsername": "Couds",
    "gitEmail": "john.elric@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/couds/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/couds/react-bulma-components/issues"
  },
  "homepage": "https://github.com/couds/react-bulma-components#readme",
