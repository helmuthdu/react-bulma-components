{
  "name": "react-bulma-components",
  "version": "3.0.1-3",
  "description": "React components for Bulma framework",
  "main": "dist",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "eslint . && jest --coverage",
    "test:clean": "rm -rf $(find ./lib -name \"__tests__\")",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-ghpages",
    "prepare": "npm run build:clean && npm run build && npm run test:clean",
    "build": "webpack --progress --color --mode=production && npm run build:lib",
    "build:lib": "NODE_ENV=production tsc --resolveJsonModule --declaration --esModuleInterop --jsx preserve --strict --target ES2020 --module commonjs --outDir lib src/index.ts",
    "build:clean": "rm -rf dist && rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helmuthdu/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helmuthdu/react-bulma-components/issues"
  },
  "homepage": "https://github.com/helmuthdu/react-bulma-components#readme",
  "dependencies": {
    "bulma": "^0.9.0",
    "clsx": "^1.1.1",
    "react-app-polyfill": "^1.0.6",
    "react-paginate": "^6.3.2"
  },
  "peerDependencies": {
    "prop-types": ">=15.7.2",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@mdi/font": "^5.5.55",
    "@storybook/addon-a11y": "^6.0.16",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-events": "^6.0.16",
    "@storybook/addon-knobs": "^6.0.16",
