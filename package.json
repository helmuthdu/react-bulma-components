{
  "name": "react-bulma-components",
  "version": "3.0.1-3",
  "description": "React components for Bulma framework",
  "main": "dist",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "eslint . && jest --coverage",
    "test:clean": "rm -rf $(find ./lib -name \"__tests__\")",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-ghpages",
    "prepare": "npm run build:clean && npm run build && npm run test:clean",
    "build": "webpack --progress --color --mode=production && npm run build:lib",
    "build:lib": "NODE_ENV=production tsc --resolveJsonModule --declaration --esModuleInterop --jsx react --strict --target ES2020 --module commonjs --outDir lib src/index.ts",
    "build:clean": "rm -rf dist && rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helmuthdu/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helmuthdu/react-bulma-components/issues"
  },
  "homepage": "https://github.com/helmuthdu/react-bulma-components#readme",
  "dependencies": {
    "bulma": "^0.9.1",
    "clsx": "^1.1.1",
    "react-app-polyfill": "^2.0.0",
    "react-paginate": "^6.5.0"
  },
  "peerDependencies": {
    "prop-types": ">=15.7.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@mdi/font": "^5.8.55",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-events": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storyshots": "^6.0.28",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/addon-viewport": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/cli": "^6.0.28",
    "@storybook/components": "^6.0.28",
    "@storybook/core-events": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@storybook/storybook-deployer": "^2.8.7",
    "@storybook/theming": "^6.0.28",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.5",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-paginate": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
