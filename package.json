{
  "name": "react-bulma-components",
  "version": "4.0.0",
  "description": "React components for Bulma framework",
  "main": "dist",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "eslint . && jest --coverage",
    "test:clean": "rm -rf $(find ./lib -name \"__tests__\")",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-ghpages",
    "prepare": "npm run build:clean && npm run build && npm run test:clean",
    "build": "webpack --progress --color --mode=production && npm run build:lib",
    "build:lib": "NODE_ENV=production tsc --resolveJsonModule --declaration --esModuleInterop --jsx preserve --strict --target es5 --module commonjs --outDir lib src/index.ts",
    "build:clean": "rm -rf dist && rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helmuthdu/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helmuthdu/react-bulma-components/issues"
  },
  "homepage": "https://github.com/helmuthdu/react-bulma-components#readme",
  "dependencies": {
    "bulma": "^0.9.2",
    "clsx": "^1.1.1",
    "react-app-polyfill": "^2.0.0",
    "react-paginate": "^7.1.0"
  },
  "peerDependencies": {
    "prop-types": ">=15.7.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@mdi/font": "^5.9.55",
    "@storybook/addon-a11y": "^6.1.20",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-events": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-storyshots": "^6.1.20",
    "@storybook/addon-storysource": "^6.1.20",
    "@storybook/addon-viewport": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/cli": "^6.1.20",
    "@storybook/components": "^6.1.20",
    "@storybook/core-events": "^6.1.20",
    "@storybook/node-logger": "^6.1.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.20",
    "@storybook/theming": "^6.1.20",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-paginate": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "coveralls": "^3.1.0",
    "css-loader": "^5.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-webpack-plugin": "^2.5.2",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.3.9",
